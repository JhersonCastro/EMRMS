<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="EMRMS.Utilities.Videos.VideoHelper"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:EMRMS.Utilities.Videos"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"

    mc:Ignorable="d"
    Title="WinUI MediaPlayer Example">

    <Grid>
        <WebView2
            x:Name="webViewer" HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            IsRightTapEnabled="False"
            CoreWebView2Initialized="webViewer_CoreWebView2Initialized"
            ></WebView2>
    </Grid>
</Window>
